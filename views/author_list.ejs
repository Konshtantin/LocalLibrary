<%- include('./partials/head') %> 
<body>
    <div class="app">
        <%- include('./partials/sidebar') %> 
        <div class="content">
            <h1><%= title %> </h1>

            <ul>
                <% if(author_list.length > 0) { %>
                    <% author_list.forEach(author => { %>
                        <li>
                            <a href="<%= author.url %> "><%= author.name %> </a> |
                            (<%= author.dob %> - <%= author.dod %>)
                        </li>
                    <% }) %> 
                <% } else { %>
                    <li>There are no authors</li>
                <% } %>   
            </ul>
        </div>
    </div>

</body>
</html>